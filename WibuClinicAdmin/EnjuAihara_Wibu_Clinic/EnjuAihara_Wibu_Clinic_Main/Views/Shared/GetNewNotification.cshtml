@model EnjuAihara.EntityFramework.NotificationModel
<li class="list-group-item">
	<div class="timeline-item text-left">
		<span class="time"><i class="fas fa-clock"></i> 27 mins ago</span>
		<h3 class="timeline-header">@Model.NotificationTitle</h3>
		<div class="timeline-body">
			@Model.NotificationDetail
		</div>
		<div class="timeline-footer">
			<a class="btn btn-warning btn-sm" href="@Model.NotificationLink">Xem chi tiết</a>
		</div>
	</div>
</li>